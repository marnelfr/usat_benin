<div class="modal-content">
  {{ form_start(form, {action: path('remover_new'), id: 'modal_form_remover_new'}) }}
  <div class="modal-header">
    <h4 class="modal-title"><i class="fas fa-plus-circle"></i> Ajout d'Enleveur</h4>
    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
      <span aria-hidden="true">&times;</span></button>
  </div>
  <div class="modal-body">
    {{ form_widget(form) }}
  </div>
  <div class="modal-footer text-left">
    <button type="reset" class="btn btn-default" data-dismiss="modal">Fermer</button>
    <button id="modal-remover-saver" class="btn btn-success float-right"><i class="fas fa-save"></i> Enregistrer</button>
  </div>
  {{ form_end(form) }}
</div>
{#{% block javascripts %}#}
  {#{{ encore_entry_script_tags('importer') }}#}
{#{% endblock %}#}
